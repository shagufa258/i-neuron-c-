//*******10. Write a C++ program to add all the numbers of an array of size 10.*********
#include <iostream>
using namespace std;

int main()
{   int a[10], sum=0, inp;
    cout<<"enter 10 num- ";
    for(int i=0; i<10; i++)
    {
       cin>>a[i];
       sum+=a[i];
    }
    cout<<"sum = "<<sum;
}


